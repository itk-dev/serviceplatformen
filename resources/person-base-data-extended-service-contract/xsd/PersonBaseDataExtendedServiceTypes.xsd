        <xsd:sequence>
            <xsd:element name="status" type="tns:statusTypeType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="statusdato" type="tns:usikkerhedDatoTidType" minOccurs="0" maxOccurs="1"/>
