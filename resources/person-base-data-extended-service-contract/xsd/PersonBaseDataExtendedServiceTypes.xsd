    <xsd:simpleType name="foraeldremyndighedTypeType">
        <xsd:annotation>
            <xsd:documentation>
                3 = Personnummer forældremyndighed mor
                4 = Personnummer forældremyndighed far
                5 = Anden indehaver 1
                6 = Anden indehaver 2
            </xsd:documentation>
        </xsd:annotation>
